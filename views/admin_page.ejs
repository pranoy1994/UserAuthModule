<% include ./partials/header.ejs %>
	<div>
		<h1>ApolloMail</h1>
		<p>Manage all the users of Apollo Mail here.</p>
	</div>
	<% Users.forEach(user){ %>
		<div class='row'>
			<div class="col-lg-3">
				<%= user.name %>
			</div>
			<div class="col-lg-3">
				<%= user.email %>
			</div>
			<div class="col-lg-3">
				<%= user.isAuthenticated %>
			</div>
			<div class="col-lg-3">
				<a href="#">Delete User</a>
			</div>
		</div>
	<% } %>
	</div>
<% include ./partials/footer.ejs %>